{
    "name": "silktide/brightlocal-api",
    "description": "BrightLocal API wrapper.",
    "type": "library",
    "license": "GPL-3.0+",
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~6",
        "codeclimate/php-test-reporter": "^0.3.2"
    },
    "authors": [
        {
            "name": "Andrew Waite",
            "email": "andrewwaite@silktide.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Silktide\\BrightLocalApi\\": "src",
            "Silktide\\BrightLocalApi\\Test\\": "test"
        }
    },
    "extra": {
        "downsider-puzzle-di": {
            "silktide/syringe": {
                "path": "config/syringe.yml"
            }
        }
    }
}
